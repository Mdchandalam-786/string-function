    <script>
       var person=[
  {
    "id": 1,
    "title": "My First Blog Post",
    "body": "This is the content...",
    "author": "Alice"
  },
  {
    "id": 2,
    "title": "Another Post",
    "body": "More content here...",
    "author": "Bob"
  }
]
    document.write(person[0].title,"<br>");
    document.write(person[1].title.length,"<br>");
    document.write(person[1].author,"<br>");
    document.write(person.length);

    </script>
</body>
